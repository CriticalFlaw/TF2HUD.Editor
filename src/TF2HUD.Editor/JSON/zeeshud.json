  "Screenshots": [
    "https://i.imgur.com/yZCBMhF.jpeg",
    "https://i.imgur.com/AwM4vuu.jpeg",
    "https://i.imgur.com/MzN1XSn.jpeg",
    "https://i.imgur.com/9niNBz2.jpeg"
  ],
  "Background": "https://i.imgur.com/U4m1mOx.png",
  "Layout": [
    "0 0 4 4 1 1",
    "5 5 2 2 1 1",
    "6 6 3 3 1 1"
  ],
  "Links": {
    "Update": "https://github.com/Zeesastrous/ZeesHUD/archive/refs/heads/main.zip", // TODO: Remove
    "GitHub": "https://github.com/Zeesastrous/ZeesHUD",
    "Download": [
      {
        "Source": "GitHub",
        "Link": "https://github.com/Zeesastrous/ZeesHUD/archive/refs/heads/main.zip"
      }
    ]
  },
