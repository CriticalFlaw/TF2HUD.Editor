        "Value": "71 9 31 255",
        "Width": 170,
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "Ammo Reserve Low Shadow": "$value"
              }
            }
          }
        }
      }
    ],
    "Crosshair": [
      {
        "Name": "zh_color_xhair_normal",
        "Label": "Base Color",
        "Type": "ColorPicker",
        "ToolTip": "Default crosshair color.",
        "Value": "255 255 255 255",
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "Crosshair": "$value"
              }
            }
          }
        }
      },
      {
        "Name": "zh_color_xhair_onhit",
        "Label": "Color On Hit",
        "Type": "ColorPicker",
        "ToolTip": "Default crosshair color on player hit.",
        "Value": "255 0 90 255",
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "CrosshairDamage": "$value",
                "Hitmarker": "$value"
              }
            }
          }
        }
      },
      {
        "Name": "zh_val_xhair_size",
        "Label": "Size",
        "Type": "IntegerUpDown",
        "Value": "18",
        "Minimum": "10",
        "Maximum": "30",
        "Increment": "1",
        "Width": 150,
        "ToolTip": "Size of the crosshair.",
        "Files": {
          "#customizations/crosshairs.res": {
            "CustomCrosshair": {
              "font": "Size:$value | Outline:OFF"
            }
          }
        }
      },
      {
        "Name": "zh_val_xhair_style",
        "Label": "Style",
        "Type": "Crosshair",
        "ToolTip": "Style of crosshair.",
        "Value": "<",
        "Files": {
          "#customizations/crosshairs.res": {
            "CustomCrosshair": {
              "labelText": "$value"
            }
          }
        }
      },
      {
        "Name": "zh_toggle_xhair_enable",
        "Label": "Toggle Crosshair",
        "Type": "Checkbox",
        "Value": "false",
        "ToolTip": "Toggle crosshair visibility.",
        "Files": {
          "#customizations/crosshairs.res": {
            "CustomCrosshair": {
              "visible": {
                "true": "1",
                "false": "0"
              }
            }
          }
        }
      },
      {
        "Name": "zh_toggle_xmarker_enable",
        "Label": "Hitmarker",
        "Type": "Checkbox",
        "Value": "false",
        "ToolTip": "Toggle hitmarker visibility.",
        "Files": {
          "#customizations/hitmarker.res": {
            "CustomHitmarker": {
              "visible": {
                "true": "1",
                "false": "0"
              }
            }
          }
        }
      }
    ],
    "Damage Numbers": [
      {
        "Name": "zh_dmgnum_size",
        "Label": "Num. Size",
        "Type": "IntegerUpDown",
        "Value": "32",
        "Minimum": "16",
        "Maximum": "56",
        "Increment": "4",
        "Width": 150,
        "ToolTip": "Size of the damage numbers that show up above players.",
        "Files": {
          "#customizations/damage_counter.res": {
            "CDamageAccountPanel": {
              "delta_item_font": "Damage ($value)"
            }
          }
        }
      },
      {
        "Name": "zh_dmgnum_size_crit",
        "Label": "Size (Crit)",
        "Type": "IntegerUpDown",
        "Value": "48",
        "Minimum": "16",
        "Maximum": "56",
        "Increment": "4",
        "Width": 150,
        "ToolTip": "Size of the damage numbers that show up above players, when damage is critical.",
        "Files": {
          "#customizations/damage_counter.res": {
            "CDamageAccountPanel": {
              "delta_item_font_big": "Damage ($value)"
            }
          }
        }
      },
      {
        "Name": "zh_dmgnum_centered_size",
