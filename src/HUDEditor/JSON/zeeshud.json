{
  "$schema": "https://raw.githubusercontent.com/CriticalFlaw/TF2HUD.Editor/master/src/HUDEditor/JSON/Schema/schema.json",
  "Author": "Zeesastrous",
  "Description": "ZeesHUD is a stylish and ambitious HUD inspired by retro punk aesthetics.",
  "Thumbnail": "https://i.imgur.com/RC89MAt.png",
  "Screenshots": [
    "https://i.imgur.com/TpWc1sD.jpeg",
    "https://i.imgur.com/LnmdopX.jpeg",
    "https://i.imgur.com/HMneh83.jpeg",
    "https://i.imgur.com/kQ3Awsc.jpeg"
  ],
  "Background": "https://i.imgur.com/U4m1mOx.png",
  "Layout": [
    "0 0 4 4 3 3",
    "5 5 2 2 3 3",
    "1 1 1 1 1 1"
  ],
  "Links": {
    "Update": "https://github.com/Zeesastrous/ZeesHUD/archive/refs/heads/main.zip",
    "GitHub": "https://github.com/Zeesastrous/ZeesHUD",
    "TF2Huds": "https://tf2huds.dev/hud/ZeesHUD",
    "ComfigHuds": "https://comfig.app/huds/page/zeeshud/"
  },
  "CustomizationsFolder": "#customizations//addons",
  "EnabledFolder": "#customizations//addons//_enabled",
  "Controls": {
    "HUD Layout": [
      {
        "Name": "zh_val_layout_style",
        "Label": "Layout Style",
        "Type": "ComboBox",
        "Value": "0",
        "Width": 350,
        "ToolTip": "Layout of health, ammo, objective, meters, etc. in-game.",
        "ComboFiles": [
          "health-ammo-style_ut",
          "health-ammo-style_classic"
        ],
        "Options": [
          {
            "Label": "Default",
            "Value": "0"
          },
          {
            "Label": "Unreal Tournament (UT Mode)",
            "Value": "1",
            "FileName": "health-ammo-style_ut"
          },
          {
            "Label": "Classic",
            "Value": "2",
            "FileName": "health-ammo-style_classic"
          }
        ]
      },
      {
        "Name": "zh_accent_color",
        "Label": "Accent Color",
        "Type": "ColorPicker",
        "ToolTip": "Main color for ZeesHUD. Also affects certain elements.",
        "Value": "255 0 90 255",
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "ZeesMain": "$value"
              }
            }
          }
        }
      },
      {
        "Name": "zh_val_layout_font",
        "Label": "Font",
        "Type": "ComboBox",
        "Value": "0",
        "Width": 190,
        "ToolTip": "Font used for Health, Ammo and Counters",
        "ComboFiles": [
          "health-ammo_font_futura"
        ],
        "Options": [
          {
            "Label": "CIKANDEI (Default)",
            "Value": "0"
          },
          {
            "Label": "Futura Bold",
            "Value": "1",
            "FileName": "health-ammo_font_futura"
          }
        ]
      }
    ],
    "Health & Ammo Colors": [
      {
        "Name": "zh_color_health_normal",
        "Label": "HP Normal",
        "Type": "ColorPicker",
        "ToolTip": "Color of player health when normal.",
        "Value": "255 255 255 255",
        "Width": 170,
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "Health Text": "$value"
              }
