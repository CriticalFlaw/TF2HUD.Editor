    "Addons": [
      {
        "Name": "zh_centered_uber",
        "Label": "Centered Uber",
        "Type": "Checkbox",
        "Value": "false",
        "ToolTip": "Centers the uber meter around the crosshair.",
        "FileName": "uber_centered"
      },
      {
        "Name": "zh_friendslist",
        "Label": "Show Friends List",
        "Type": "Checkbox",
        "Value": "true",
        "ToolTip": "Shows friends list in the main menu.",
        "FileName": "mainmenu_enablefriends"
      },
      {
        "Name": "zh_centered_meters",
        "Label": "Centered Meters",
        "Type": "Checkbox",
        "Value": "false",
        "ToolTip": "Centers the item meter above objectives.",
        "FileName": "meters_centered"
