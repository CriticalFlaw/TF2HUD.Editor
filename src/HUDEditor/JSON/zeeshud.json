        "Label": "Heal Color",
        "Type": "ColorPicker",
        "ToolTip": "Color of the damage number when heal.",
        "Value": "0 255 214 255",
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "Damage Heal": "$value"
              }
            }
          }
        }
      },
      {
        "Name": "zh_dmgnum_centered",
        "Label": "Centered Color",
        "Type": "ColorPicker",
        "ToolTip": "Color of the damage number in the center.",
        "Value": "255 255 255 255",
        "Files": {
          "#customizations/custom_colors.res": {
            "Scheme": {
              "Colors": {
                "Damage Number": "$value"
              }
            }
          }
        }
      },
      {
        "Name": "zh_toggle_centered_dmgnum",
        "Label": "Centered Damage Numbers",
        "Type": "Checkbox",
        "Value": "true",
        "ToolTip": "Toggle centered damage number visibility.",
        "Files": {
          "#customizations/damage_counter.res": {
            "DamageAccountValue": {
              "visible": {
                "true": "1",
                "false": "0"
              },
              "enabled": {
                "true": "1",
                "false": "0"
              }
            }
          }
        }
      }
